{
  "name": "pangea-webcomponents",
  "version": "1.0.0",
  "scripts": {
    "build:browser": "parcel build --target browsers",
    "build:lib": "parcel build --target library",
    "build": "npm run build:browser && npm run build:lib"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@mui/styled-engine-sc": "^6.0.0-alpha.17",
    "@mui/types": "^7.2.13",
    "@pangeacyber/react-mui-audit-log-viewer": "^0.0.69",
    "@pangeacyber/react-mui-branding": "0.0.18",
    "@pangeacyber/react-mui-store-file-viewer": "^0.0.44",
    "@preact/compat": "^17.1.2",
    "preact": "^10.22.0",
    "styled-components": "^6.1.8"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "targets": {
    "browsers": {
      "source": ["src/audit-log-viewer.tsx", "src/store-file-viewer.tsx"],
      "distDir": "./dist/browser",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "outputFormat": "global"
    },
    "library": {
      "source": ["src/audit-log-viewer.tsx", "src/store-file-viewer.tsx"],
      "distDir": "./dist/lib",
      "context": "node",
      "outputFormat": "esmodule"
    }
  }
}
